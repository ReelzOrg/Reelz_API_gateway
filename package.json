{
	"name": "reelz_server",
	"version": "1.0.0",
	"engines": {
		"node": ">=22.15.0"
	},
	"description": "Server for the Reelz App",
	"keywords": [
		"Reelz",
		"Short Videos",
		"Social Media"
	],
	"license": "ISC",
	"author": "Viraj Doshi",
	"type": "module",
	"main": "app.js",
	"scripts": {
		"start": "node --import ./instrumentation.js app.js",
		"proto:setup": "git submodule update --remote --merge && buf dep update protos && buf lint protos",
		"proto:gen": "buf generate protos",
		"proto:types": "./node_modules/.bin/proto-loader-gen-types --keepCase --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=generated/protos/llm_service.proto",
		"preinstall": "npx only-allow pnpm",
		"docker:up": "docker-compose up -d",
		"start:full": "npm run docker:up && npm run start",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
		"test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --coverage"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.775.0",
		"@aws-sdk/lib-storage": "^3.830.0",
		"@aws-sdk/s3-request-presigner": "^3.775.0",
		"@bufbuild/protobuf": "^2.11.0",
		"@clickhouse/client": "^1.12.1",
		"@confluentinc/kafka-javascript": "^1.4.0",
		"@grpc/grpc-js": "^1.13.4",
		"@grpc/proto-loader": "^0.7.15",
		"@kafkajs/confluent-schema-registry": "^3.9.0",
		"@neo4j/graphql-ogm": "^5.11.4",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/auto-instrumentations-node": "^0.64.1",
		"@opentelemetry/exporter-logs-otlp-http": "^0.205.0",
		"@opentelemetry/instrumentation-pino": "^0.52.0",
		"@opentelemetry/resources": "^2.1.0",
		"@opentelemetry/sdk-logs": "^0.205.0",
		"@opentelemetry/sdk-metrics": "^2.1.0",
		"@opentelemetry/sdk-node": "^0.205.0",
		"@opentelemetry/sdk-trace-node": "^2.1.0",
		"@opentelemetry/semantic-conventions": "^1.37.0",
		"@toon-format/toon": "^2.0.1",
		"@valkey/valkey-glide": "^2.2.1",
		"bcryptjs": "^3.0.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"google-auth-library": "^10.2.1",
		"grpc-tools": "^1.13.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.2",
		"neo4j-driver": "^5.28.1",
		"pg": "^8.14.1",
		"pino": "^9.11.0",
		"pino-http": "^10.5.0",
		"redlock": "5.0.0-beta.2",
		"typesense": "^2.0.3",
		"ultimate-express": "^2.0.15",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@bufbuild/buf": "^1.64.0",
		"@google-cloud/local-auth": "^3.0.1",
		"@tsconfig/node-lts": "^22.0.3",
		"@types/body-parser": "^1.19.6",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/express-serve-static-core": "^5.1.1",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/multer": "^2.0.0",
		"@types/node": "^24.3.0",
		"@types/pg": "^8.11.11",
		"@types/redlock": "^4.0.8",
		"@types/supertest": "^6.0.3",
		"googleapis": "^159.0.0",
		"jest": "^30.1.1",
		"jest-cli": "^30.1.3",
		"pino-pretty": "^13.1.3",
		"supertest": "^7.1.4",
		"ts-proto": "^2.11.1",
		"typescript": "^5.9.3"
	}
}